        <dependency>
            <groupId>com.googles</groupId>
            <artifactId>protobuf</artifactId>
            <scope>system</scope>
            <version>2.4.1</version>
            <systemPath>${pom.basedir}/lib/protobuf-java-2.4.1.jar</systemPath>
        </dependency>

        <dependency>
            <groupId>ion.front</groupId>
            <artifactId>serialer</artifactId>
            <scope>system</scope>
            <version>1.0.0</version>
