            <scope>system</scope>
            <version>1.0</version>
            <systemPath>${pom.basedir}/lib/ojdbc14.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>oracle.jdbc</groupId>
            <artifactId>oracle.jdbc</artifactId>
            <scope>system</scope>
            <version>1.0</version>
            <systemPath>${pom.basedir}/lib/oracle.jdbc_10.2.0.jar</systemPath>
        </dependency>
