        </dependency>

        <dependency>
            <groupId>com.aliyun.aliscom</groupId>
            <artifactId>backup</artifactId>
            <version>0.0.1-SNAPSHOT</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/lib/com.tl.protocols.sg645.backup-0.0.1-SNAPSHOT.jar
            </systemPath>
        </dependency>

        <dependency>
            <groupId>com.aliyun.aliscom</groupId>
