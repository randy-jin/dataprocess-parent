    <!--datahub-->
    <bean id="datahubClient" class="com.aliyun.datahub.DatahubClient">
        <constructor-arg name="conf" ref="datahubConfiguration"/>
    </bean>
    <bean id="datahubConfiguration" class="com.aliyun.datahub.DatahubConfiguration">
        <constructor-arg name="account" ref="aliyunAccount"/>
        <constructor-arg name="endpoint" value="${endpoint:apollo}"/>
    </bean>
    <bean id="aliyunAccount" class="com.aliyun.datahub.auth.AliyunAccount">
        <constructor-arg name="accessId" value="${accessId:apollo}"/>
        <constructor-arg name="accessKey" value="${accessKey:apollo}"/>
    </bean>
